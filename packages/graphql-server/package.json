{
  "name": "graphql-server",
  "version": "1.0.0",
  "description": "GraphQL server for react-drums",
  "main": "server.js",
  "scripts": {
    "build": "pnpm i && npx prisma generate",
    "dev": "ts-node-dev src/server.ts",
    "init": "npx prisma migrate dev --name init",
    "migrate": "prisma migrate",
    "seed": "ts-node-dev prisma/seed-db.ts",
    "studio": "npx prisma studio"
  },
  "keywords": [
    "GraphQL",
    "Server"
  ],
  "author": "Mannuel Ferreira",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^20.10.6",
    "core": "link:@types@pothos/core",
    "prisma": "5.7.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@pothos/core": "^3.41.0",
    "@pothos/plugin-prisma": "^3.63.0",
    "@prisma/client": "5.7.1",
    "graphql": "^16.8.1",
    "graphql-scalars": "^1.22.4",
    "graphql-yoga": "^5.1.0"
  }
}
