datasource db {
        provider = "sqlite"
        url      = "file:./dev.db"
}

generator client {
        provider = "prisma-client-js"
}

model User {
        id        Int      @id @default(autoincrement())
        firstName String
        lastName  String
        email     String   @unique
        password  String
        createdAt DateTime @default(now())
}
