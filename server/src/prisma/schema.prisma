datasource db {
        provider = "sqlite"
        url      = "file:./dev.db"
}

generator client {
        provider = "prisma-client-js"
}

model User {
        id        Int      @id @default(autoincrement())
        firstName String
        lastName  String
        email     String   @unique
        password  String
        createdAt DateTime @default(now())
        updatedAt DateTime @updatedAt
}

model Kit {
        id   Int    @id @default(autoincrement())
        name String
}

model Sound {
        id      Int    @id @default(autoincrement())
        name    String
        src     String
        keyCode Int
        keyChar String
}
