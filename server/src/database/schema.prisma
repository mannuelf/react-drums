datasource db {
        provider = "sqlite"
        url      = env("DATABASE_URL")
}

generator client {
        provider = "prisma-client-js"
}

model User {
        id        Int      @id @default(autoincrement())
        firstName String   @map(name: "first_name")
        lastName  String   @map(name: "last_name")
        email     String   @unique
        password  String
        createdAt DateTime @default(now()) @map(name: "created_at")
}
